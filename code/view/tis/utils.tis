//生成语言
function createLang(){
    //view.msgbox(#alert,"createlang");
    var content = $(body).html;
    for(var key in CONF.Lang) {
        content = content.replace("{{"+ key +"}}",CONF.Lang[key]);
    }
    $(body).html = content;
}

//弹出错误窗口
function errorBox(str){
    view.msgbox(#error,str);
}
